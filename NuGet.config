<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="NuGetFeed" value="{NUGET_SOURCE}" />
    </packageSources>
    <packageSourceCredentials>
        <NuGetFeed>
            <add key="Username" value="{NUGET_USERNAME}" />
            <add key="ClearTextPassword" value="{NUGET_API_KEY}" />
        </NuGetFeed>
    </packageSourceCredentials>
</configuration>